{
  "extends": [
    "prettier",
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "plugins": ["prettier", "react"],
  "rules": {
    "prettier/prettier": ["error"],
    "react/prop-types": 0
  },
  "parser": "babel-eslint",
  "settings": {
    "react": {
        "version": "detect"
    }
  }
}

